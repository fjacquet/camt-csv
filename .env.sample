# API Keys
GEMINI_API_KEY=your-google-api-key-here

# AI Configuration
CAMT_AI_ENABLED=true
CAMT_AI_MODEL=gemini-2.0-flash

# Logging Configuration
# Available log levels: trace, debug, info, warn, error, fatal, panic
CAMT_LOG_LEVEL=info

# Available log formats: text, json
CAMT_LOG_FORMAT=text

# Data directory path (optional)
# CAMT_DATA_DIRECTORY=/path/to/data/directory

# CSV delimiter for output files (default is ',')
CAMT_CSV_DELIMITER=;